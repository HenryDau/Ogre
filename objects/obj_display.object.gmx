            <string>if (global.selected_display != id &amp;&amp; !global.paused){
    global.unit_selected = this_unit;
    
    var spr;
    if (this_unit == "None")
        spr = noone
    else
        spr = sprite_index
        
    global.selected_display.sprite_index = spr;
    
    if (instance_number(obj_result) &gt; 0)
        with (obj_result)
            instance_destroy();
    
    if (global.turn == "AA"){
        if (!has_shot){
            selected = !selected;
            if (selected){
                outline.visible = true;
                if (global.combined_attack_range &gt; range || global.combined_attack_range == 0){
                    global.combined_attack_range = range;
                } //else 
                    //show_debug_message("Range: " + string(range) + " Now why didn;t it change");
            } else {
                global.combined_attack_range = 0;
                with(obj_display){
                    if (selected){
                        if (global.combined_attack_range &gt; range || global.combined_attack_range == 0){
                            global.combined_attack_range = range;
                        }
                    }
                }
                outline.visible = false;
            }
        }
        
        if (global.combined_attack_range != 0){
            if (global.target != noone)
                obj_attack_button.visible = true;
            else
                obj_attack_button.visible = false;
            show_valid_targets();
        } else {
            obj_attack_button.visible = false;
            obj_hex.sprite_index = spr_hex;
        }
        
        // Remove target if selecting a new weapon puts it out of range
        if (global.target != noone){
            var tile = get_tile_at(global.target.x, global.target.y);
            if (tile.sprite_index != spr_valid_hex){
                global.target = noone;   
                obj_attack_button.visible = false;
            }
        }
    } else if (global.turn == "DA"){
        global.target = id;
    }
}
